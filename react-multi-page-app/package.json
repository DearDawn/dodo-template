  "license": "ISC",
  "devDependencies": {
    "@parcel/config-default": "^2.5.0",
    "@parcel/core": "^2.5.0",
    "@parcel/transformer-less": "^2.5.0",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "parcel": "^2.5.0"
  },
  "dependencies": {
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  }
}